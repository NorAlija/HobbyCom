{
  "expo": {
    "name": "h-com",
    "slug": "h-com",
    "scheme": "h-com",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/app/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/app/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.basel123.hcom"
    },
    "web": {
      "favicon": "./src/app/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cc67523b-be42-462a-8ce7-062f43bb4859"
      }
    }
  }
}
